{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
          "title": { "type": "string" },
          "authors": {
            "type": "array",
            "items": { "type": "string" }
          },
          "venue": { "type": "string" },
          "date": { "$ref": "#/definitions/date" },
          "links": {
            "type": "object"
          },
          "preprint": {
            "const": true
          }
        },
        "required": [
          "title",
          "authors",
          "venue",
          "date"
        ]
    },
    "definitions": {
        "date": {
            "type": "string",
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$",
            "example": "2019-05-17"
        }
    }
}
